{
    "name": "agungsugiarto/laravel-route-discovery",
    "description": "Auto register routes using PHP attributes",
    "keywords": [
        "spatie",
        "laravel-route-discovery"
    ],
    "homepage": "https://github.com/agungsugiarto/laravel-route-discovery",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Agung Sugiarto",
            "email": "me.agungsugiarto@gmail.com",
            "homepage": "https://agungsugiarto.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/contracts": "^8.0|^9.0",
        "illuminate/support": "^8.0|^9.0",
        "spatie/laravel-package-tools": "^1.10",
        "symfony/finder": "^5.4.2|^6.0",
        "symfony/polyfill-php80": "^1.26"
    },
    "require-dev": {
        "nunomaduro/larastan": "^1.0.2",
        "orchestra/testbench": "^6.23.2|^7.0",
        "pestphp/pest": "^1.21",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "rector/rector": "^0.14.0",
        "spatie/laravel-ray": "^1.27"
    },
    "autoload": {
        "psr-4": {
            "Fluent\\RouteDiscovery\\": "src",
            "Fluent\\RouteDiscovery\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fluent\\RouteDiscovery\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest --colors=always",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "Fluent\\RouteDiscovery\\RouteDiscoveryServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
